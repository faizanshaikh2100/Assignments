public class Day2_ConstructorTypes{
    public Day2_ConstructorTypes(int a,int b){
        System.out.println("2 args constructor :"+(a+b));
    }
    public Day2_ConstructorTypes(float a,int b){
        System.out.println("2 args constructor with float :"+(a+b));
    }
    public Day2_ConstructorTypes(){
        System.out.println("0 args constructor");
    }
    public void add(){
        
    }
    public static void main(String[] args) {
        Day2_ConstructorTypes ct = new Day2_ConstructorTypes();
        Day2_ConstructorTypes ctadd = new Day2_ConstructorTypes(2,3);
    }
}